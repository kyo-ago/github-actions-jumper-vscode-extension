{
  "name": "github-actions-jumper",
  "displayName": "GitHub Actions Jumper",
  "description": "Jump to GitHub Actions referenced files with Ctrl+Click",
  "version": "0.0.6",
  "publisher": "kyo-ago",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "keywords": ["github", "actions", "yaml", "workflow", "navigation"],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyo-ago/github-actions-jumper-vscode-extension.git"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "contributes": {},
  "scripts": {
    "package": "vsce package",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.3.0"
  },
  "dependencies": {
    "yaml": "^2.3.0"
  }
}